			<causes>
				<option>
					<causes>
						<port id="portA" specType="org.bip.spec.ExampleA" />
						<port id="portA" specType="org.bip.spec.ExampleA" />
						<port id="portA" specType="org.bip.spec.ExampleA" />
						<port id="portC" specType="org.bip.spec.ExampleC" />
